{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/saengsanso/all",
      "dest": "/backend/server.js",
      "headers": {
        "Cache-Control": "public, s-maxage=300, stale-while-revalidate=600"
      }
    },
    {
      "src": "/api/saengsanso/(exhibitions|projects|news|archive|slides|about-page|members)",
      "dest": "/backend/server.js",
      "headers": {
        "Cache-Control": "public, s-maxage=300, stale-while-revalidate=600"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/logo.svg",
      "dest": "/logo.svg",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/logo.png",
      "dest": "/logo.png",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/googlee27a3f9e329bcf11.html",
      "dest": "/googlee27a3f9e329bcf11.html"
    },
    {
      "src": "/pdf/(.*)",
      "dest": "/pdf/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/fonts/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
